{
  "compilerOptions": {
    "target": "ES6",
    "module": "ES6",
    "moduleResolution": "Node",
    "declaration": false,
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": false,
    "sourceMap": true,
    "noLib": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "outDir": "dist-prueba"
  },
  "exclude": ["node_modules"]
}
